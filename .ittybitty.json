{
  "model": "opus",
  "fps": 10,
  "permissions": {
    "manager": {
      "allow": [
        "Read",
        "Write",
        "Edit",
        "Glob",
        "Grep",
        "Bash(jq:*)",
        "Bash(wc:*)",
        "Bash(chmod +x tests/:*)",
        "Bash(chmod +x ./tests/:*)",
        "Bash(./tests/*.sh:*)",
        "Bash(sleep:*)"
      ],
      "deny": []
    },
    "worker": {
      "allow": [
        "Read",
        "Write",
        "Edit",
        "Bash"
      ],
      "deny": []
    }
  },
  "externalDiffTool": "\"git webdiff\""
}
